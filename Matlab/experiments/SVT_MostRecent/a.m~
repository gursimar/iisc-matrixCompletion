%%
Y=[ 5.06 4.60 8.42 ; 0 0 0  ];
h = bar(Y,'grouped');
set(h(1),'facecolor','red') % use color name
set(h(2),'facecolor',[0 1 0]) % or use RGB triple
set(h(3),'facecolor','b') % or use a color defined in the help for PLOT
set(h,'BarWidth',1);


